{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"ignorePatterns": ["dist/**/*"],
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-explicit-any": 1,
		"@typescript-eslint/no-non-null-assertion": 1
	},
	"root": true
}
